<block a:if="{{menuTemplateList.length}}"><view class="my-tree"><view class="my-tree-item" a:for="{{menuTemplateList}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{item.children&&item.children.length}}"><view data-event-opts="{{[['tap',[['changeExpend',['$0'],[[['menuTemplateList','',index]]]]]]]}}" class="my-tree-item-subtitle" style="{{'padding-left:'+(item.level*15+'rpx')+';'}}" onTap="__e"><text class="title">{{item.title}}</text><text class="{{(('arrow iconfont icon-houtui')+' '+((item.expend)?'arrow-down':''))}}"></text></view><block a:if="{{item.expend}}"><view><block a:if="{{item.expend}}"><my-tree vue-id="{{'79d84118-1-'+index}}" menuTemplate="{{item.children}}" menuNode="{{menuNode}}" onVueInit="__l"></my-tree></block></view></block></block><block a:else><view data-event-opts="{{[['tap',[['checkItem',['$0'],[[['menuTemplateList','',index]]]]]]]}}" class="my-tree-item-value" style="{{'padding-left:'+(item.level*15+'rpx')+';'}}" catchTap="__e"><text>{{item.title}}</text></view></block></view></view></block>