.form-box {
  padding: 20rpx;
}
.form-box .title {
  font-size: 32rpx;
  padding: 10rpx 0;
}
.form-box button {
  margin-bottom: 10rpx;
}
.form-box .checkbox-list {
  width: 100%;
}
.form-box .checkbox-list label {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  margin-bottom: 10rpx;
}
.form-box .radio-container {
  width: 100%;
}
.form-box .radio-container label {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  margin-bottom: 10rpx;
}

