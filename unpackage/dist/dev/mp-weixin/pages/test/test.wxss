.test .show-date {
  width: 100%;
  border: 1rpx solid #eee;
}
.test .show-date .value {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  box-sizing: border-box;
  padding-left: 20rpx;
}
.test .show-date .empty {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  box-sizing: border-box;
  padding-left: 20rpx;
  color: #ccc;
}

