<view class="form"><view class="form-box"><view class="title">button</view><button type="primary" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['getuserinfo',['$event']]]]]}}" onGetuserinfo="__e">getUserInfo</button><button type="primary" open-type="getAuthorize" scope="phoneNumber" data-event-opts="{{[['getAuthorize',[['$onAliGetAuthorize',['getPhoneNumber','$event']]]],['error',[['$onAliAuthError',['getPhoneNumber','$event']]]]]}}" onGetAuthorize="__e" onError="__e">getPhoneNumber</button><button type="primary" data-event-opts="{{[['tap',[['openSetting',['$event']]]]]}}" onTap="__e">openSetting</button><button type="primary" data-event-opts="{{[['tap',[['getSetting',['$event']]]]]}}" onTap="__e">getSetting</button><view class="title">登录相关</view><button type="primary" data-event-opts="{{[['tap',[['goLogin',['$event']]]]]}}" onTap="__e">买东西(必须登录)</button><view class="title">跳转</view><button type="primary" data-event-opts="{{[['tap',[['goList',['$event']]]]]}}" onTap="__e">前往列表</button><view class="title">checkbox</view><block a:if="{{checkboxs.length&&checkboxsShow}}"><view class="checkbox-list"><checkbox-group data-event-opts="{{[['change',[['checkboxChange',['$event']]]]]}}" onChange="__e"><label a:for="{{checkboxs}}" a:for-item="item" a:for-index="__i0__" a:key="value"><view><checkbox value="{{item.value}}" checked="{{item.checked}}"></checkbox></view><view>{{item.name}}</view></label></checkbox-group></view></block><view class="title">input</view><view class="input-container"><input class="uni-input" focus="{{true}}" placeholder="自动获得焦点"/><input class="uni-input" maxlength="10" placeholder="最大输入长度为10"/><input class="uni-input" type="number" placeholder="这是一个数字输入框"/><input class="uni-input" password="{{true}}" type="text" placeholder="这是一个密码输入框"/><input class="uni-input" type="text" placeholder="测试input事件" data-event-opts="{{[['input',[['inputReadIn',['$event']]]]]}}" onInput="__e"/><input class="uni-input" type="text" placeholder="测试blur事件" data-event-opts="{{[['blur',[['inputTestBlur',['$event']]]]]}}" onBlur="__e"/></view><view class="title">radio</view><view class="radio-container"><block a:if="{{radiosShow}}"><radio-group data-event-opts="{{[['change',[['radioGroupChange',['$event']]]]]}}" onChange="__e"><label a:for="{{radios}}" a:for-item="item" a:for-index="index" a:key="value"><view><radio value="{{item.value}}" checked="{{item.value===currentRadioValue}}"></radio></view><view>{{item.name}}</view></label></radio-group></block></view><view class="title">slider</view><view><slider value="60" min="1" max="100" show-value="{{true}}" data-event-opts="{{[['change',[['sliderChange',['$event']]]]]}}" onChange="__e"></slider></view><view class="title">switch</view><view><switch checked="{{switchFlag}}" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" onChange="__e"></switch></view><view class="title">textarea</view><view><textarea placeholder="请输入多行文本框" maxlength="100" value="{{textareaValue}}"></textarea></view></view></view>